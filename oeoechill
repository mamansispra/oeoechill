game.ReplicatedStorage.Sounds.Yell.Value = "rbxassetid://7959271972"

local function playsound(id)
	local sfx = Instance.new("Sound", workspace)
	sfx.SoundId = "rbxassetid://"..tostring(id)

	game:GetService("SoundService"):PlayLocalSound(sfx)

	spawn(function()
		task.wait(sfx.TimeLength)
		sfx:Destroy()
	end)
end

function playticksound()
	local sfx = Instance.new("Sound", workspace)
	sfx.SoundId = "rbxassetid://4843088994"

	game:GetService("SoundService"):PlayLocalSound(sfx)

	spawn(function()
		task.wait(2)
		sfx:Destroy()
	end)
end

local function play_ingamesound(sfxname)
	local v = game.ReplicatedStorage.Sounds:FindFirstChild(sfxname)
	local sfx = Instance.new("Sound", nil)
	local id = v.Value

	sfx.SoundId = id

	for i,v in v:GetChildren() do
		sfx[v.Name] = v.Value
	end

	game.SoundService:PlayLocalSound(sfx)
	task.delay(15, function()
		sfx:Destroy()
	end)
end

game.ReplicatedStorage.Moves.H_UltimateEssence.MoveName.Value = "Essence Of Sumo Slap"


local Player = game.Players.LocalPlayer
local Rep = game.ReplicatedStorage
local Char = Player.Character
local Main = Player.PlayerGui.Interface.Battle.Main

Main.HeatMove.TextLabel:GetPropertyChangedSignal("Text"):Connect(function()
    if Main.HeatMove.TextLabel.Text == "Ultimate Essence" then
        if status.Heat.Value ~= 100 then
            local moveNameValue = Rep.Moves.H_UltimateEssence.MoveName.Value
            Main.HeatMove.TextLabel.Text = moveNameValue
            local Anim = Char.Humanoid:LoadAnimation(Rep.Moves.H_Whirl.Anim)
            Anim.Priority = Enum.AnimationPriority.Action4 
            Anim:Play()
            Anim:AdjustSpeed(.7)
            task.wait(1)
            playSound(8028327129,0.9)
            playSound(8028327129,0.9)
            playSound(8028327129,0.9)
            playSound(8028327129,0.9)
            playSound(8028327129,0.9)
            playSound(8028327129,0.9)
            playSound(8028327129,0.9)
            playSound(8028327129,0.9)
            playSound(8028327129,0.9)
            playSound(8587690460,1)
            playSound(8587690460,1)
            playSound(8587690460,1)
            playSound(8587690460,1)
            playSound(8587690460,1)
            playSound(8587690460,1)
            playSound(8587690460,1)
            playSound(8587690460,1)
            playSound(8587690460,1)
            wait(0.4)
            Anim:Stop()
            Anim:Destroy()
        else
            local moveNameValue = Rep.Moves.H_UltimateEssence.MoveName.Value
            Main.HeatMove.TextLabel.Text = moveNameValue
            local Anim = Char.Humanoid:LoadAnimation(Rep.Moves.H_UltimateEssence.Anim)
            Anim.Priority = Enum.AnimationPriority.Action4
            Anim:AdjustSpeed(0.9)
            Anim:Play()
            playSound(142472270,1.5)
            playSound(142472270,1.5)
            playSound(142472270,1.5)
            playSound(142472270,1.5)
            playSound(142472270,1.5)
            task.wait(.5)
            playsound(2174941179,1)
            playsound(2174941179,1)
            playsound(2174941179,1)
            playsound(2174941179,1)
            playsound(2174941179,1)
            task.wait(.5)
            playSound(6312893899,0.8)
            playSound(6312893899,0.8)
            playSound(6312893899,0.8)
            playSound(6312893899,0.8)
            playSound(6312893899,0.8)
            playSound(6312893899,0.8)
            playSound(6312893899,0.8)
            playSound(6312893899,0.8)
            playSound(6312893899,0.8)
            wait(0.2)
            Anim:Destroy()
        end

    end
end)

game.ReplicatedStorage.Moves.H_Fisticuffs.MoveName.Value = "Essence Of Komaki battery"

local Player = game.Players.LocalPlayer
local Rep = game.ReplicatedStorage
local Char = Player.Character
local Main = Player.PlayerGui.Interface.Battle.Main

Main.HeatMove.TextLabel:GetPropertyChangedSignal("Text"):Connect(function()
    if Main.HeatMove.TextLabel.Text == "Essence of Fisticuffs" then
        Main.HeatMove.TextLabel.Text = Rep.Moves.H_Fisticuffs.MoveName.Value
        local Anim = Char.Humanoid:LoadAnimation(Rep.Moves.H_Rolling.Anim)
        Anim.Priority = Enum.AnimationPriority.Action4
        Anim:AdjustSpeed(1.8)
        Anim:Play()
        play_ingamesound("Ignite")
        task.wait(0.25)
        play_ingamesound("Tigerswing")
        play_ingamesound("Slap")
        task.wait(0.22)
        play_ingamesound("Slap")
        play_ingamesound("Slap")
        task.wait(0.12)
        Anim:Destroy()
    end
end)

game.ReplicatedStorage.Moves.H_SumoSlap.MoveName.Value = "Essence of Sumo Slapping"

local Player = game.Players.LocalPlayer
local Rep = game.ReplicatedStorage
local Char = Player.Character
local Main = Player.PlayerGui.Interface.Battle.Main

Main.HeatMove.TextLabel:GetPropertyChangedSignal("Text"):Connect(function()
    if Main.HeatMove.TextLabel.Text == "Essence of FastFootwork[Back]" then
        Main.HeatMove.TextLabel.Text = Rep.Moves.H_SumoSlap.MoveName.Value
        local Anim = Char.Humanoid:LoadAnimation(Rep.Moves.H_SumoSlap.Anim)
        Anim.Priority = Enum.AnimationPriority.Action4
        Anim:Play()
        Anim:AdjustSpeed(1)
        wait(0.4)
        Anim:AdjustSpeed(2)
        wait(0.6)
        Anim:AdjustSpeed(1)
        wait(1)
        Anim:Stop()
    end
end)

Main.HeatMove.TextLabel:GetPropertyChangedSignal("Text"):Connect(function()
    if Main.HeatMove.TextLabel.Text == "Essence of Sumo Slapping" then
        wait(0.2)
        playSound(2711959413,1)
        playSound(2711959413,1)
        playSound(2711959413,1)
		task.wait(0.4)
		playSound(2711959413,1)
        playSound(2711959413,1)
        playSound(2711959413,1)
		task.wait(0.25)
		playSound(2711959413,1)
        playSound(2711959413,1)
        playSound(2711959413,1)
		task.wait(0.5)
		playSound(6312893899,0.8)
        playSound(6312893899,0.8)
        playSound(6312893899,0.8)
		task.wait(0.2)
    end
end)

