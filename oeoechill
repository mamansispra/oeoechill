v.Value = 'Ignite'


local imageLabel = Main.HeatMove
local interf = Player.PlayerGui.Interface-- Assuming you have an ImageLabel control
local lastText = imageLabel.TextLabel.Text
local lastText2 = interf.BattleStart.Text

while true do
	wait(0.1) -- Adjust the delay as needed

	local newText = imageLabel.TextLabel.Text
	if newText ~= lastText then
		lastText = newText

        if newText == "Guru Firearm Flip" then
			imageLabel.TextLabel.Text = "Fighter's Shot Shutdown"
        elseif newText == "Ultimate Essence" then
			imageLabel.TextLabel.Text = "The Hellish Essence of the Masters"
        elseif newText == "Essence of Double Head Crush" then
            imageLabel.TextLabel.Text = "Essence of Head-on Collision " 
        elseif newText == "Essence of Wreckage" then
            imageLabel.TextLabel.Text = "Essence of Raging Finisher "  
        elseif newText == "Essence of Escape" then
            imageLabel.TextLabel.Text = "Essence of Cyclonic Showoff "  
        elseif newText == "Essence of Wall Crush" then
            imageLabel.TextLabel.Text = "Essence of Ferocious Wall Smash "
        elseif newText == "Komaki Fist Reversal [Front]" then
			imageLabel.TextLabel.Text = "Fighter's Strike Retaliation [Front] "
		elseif newText == "Komaki Fist Reversal [Left]" then
			imageLabel.TextLabel.Text = "Fighter's Strike Retaliation [Left] "
		elseif newText == "Komaki Fist Reversal [Right]" then
			imageLabel.TextLabel.Text = "Fighter's Strike Retaliation [Right] "
		elseif newText == "Komaki Fist Reversal [Back]" then
			imageLabel.TextLabel.Text = "Fighter's Strike Retaliation [Back] "
		elseif newText == "Guru Spin Counter [Front]" then
			imageLabel.TextLabel.Text = "Essence of Noob/Pot Execution "
        elseif newText == "Essence of Beatdown" then
			imageLabel.TextLabel.Text = "Essence of Fistic Fury"
		elseif newText == "Essence of Chokehold" then
			imageLabel.TextLabel.Text = "Essence of Neck Snapping"
        elseif newText == "Essence of Head Press: Supine" then
			imageLabel.TextLabel.Text = "Essence of Ground Bashing: Supine"
		elseif newText == "Essence of Head Press: Prone" then
			imageLabel.TextLabel.Text = "Essence of Ground Bashing: Prone"            
        elseif newText == "Essence of Sumo Slapping" then
			imageLabel.TextLabel.Text = "Essence of Dashing Barrage" 
        elseif newText == "Essence of Torment" then
			imageLabel.TextLabel.Text = "Essence of Torment "
        elseif newText == "Essence of Full Swing" then
			imageLabel.TextLabel.Text = "Essence of Bone-Crushing Strike"
        elseif newText == "Essence of Finishing: Prone" then
			imageLabel.TextLabel.Text = "Essence of Finishing: Prone "
        elseif newText == "Essence of Punting" then
			imageLabel.TextLabel.Text = "Essence of Football Kicking"
        elseif newText == "Essence of Stomping" then
			imageLabel.TextLabel.Text = "Essence of Stomping "
        elseif newText == "Essence of Self-Destruct" then
			imageLabel.TextLabel.Text = "Essence of Self-Destruct "
        elseif newText == "Essence of Fisticuffs" then
			imageLabel.TextLabel.Text = "Essence of Tormenting Fists "
        elseif newText == "Essence of Fast Footwork [Front]" then
            if status.Style.Value == "Brawler" then
			    imageLabel.TextLabel.Text = "Essence of Nimble Counterattack [Front] "
            end    
        elseif newText == "Essence of Fast Footwork [Left]" then
            if status.Style.Value == "Brawler" then
			    imageLabel.TextLabel.Text = "Essence of Nimble Counterattack [Left] "
            end           
        elseif newText == "Essence of Fast Footwork [Back]" then
            if status.Style.Value == "Brawler" then
			    imageLabel.TextLabel.Text = "Essence of Nimble Counterattack [Back] "    
            end 
        elseif newText == "Essence of Fast Footwork [Right]" then
            if status.Style.Value == "Brawler" then
			    imageLabel.TextLabel.Text = "Essence of Nimble Counterattack [Right] "
            end    
        elseif newText == "Essence of Weaponry" then
            if status.Style.Value == "Blade" or "OneHandedBlade" then
                imageLabel.TextLabel.Text = "Essence of Weaponry "
            elseif status.Style.Value == "BatHolding" then
			    imageLabel.TextLabel.Text = "Essence of Weaponry [Blunt]"            
            elseif status.Style.Value == "Holding" or "BeastMedHolding" or "HeavyHammer" then
			    imageLabel.TextLabel.Text = "Essence of Weaponry [Medium]"            
            elseif status.Style.Value == "OneHandedHolding" then
			    imageLabel.TextLabel.Text = "Essence of Weaponry [Small]"            
            elseif status.Style.Value == "HeavyHolding" or "BeastHolding" then
			    imageLabel.TextLabel.Text = "Essence of Weaponry [Heavy]"            
            elseif status.Style.Value == "Tonfa" then
			    imageLabel.TextLabel.Text = "Essence of Weaponry [Tonfa]"            
            elseif status.Style.Value == "RPG" or "Pistol" or "SMG" or "Pistol" or "Rifle" or "Shotgun" or "Sniper" then
			    imageLabel.TextLabel.Text = "Essence of Weaponry [Firearm]"            
            end                         
        end
    end

    local newText2 = interf.BattleStart.Text
    if newText2 ~= lastText2 then
		lastText2 = newText2

        if newText2 == "LEGENDARY DRAGON" then
            local r = math.random(1, 3)
			interf.BattleStart.Text = r == 1 and "'THE DRAGON OF DOJIMA'" or r == 2 and "'KAZUMA KIRYU'" or "LEGENDARY DRAGON"
		elseif newText2 == "MYSTERIOUS MERCS" then
            local r = math.random(1, 2)
			interf.BattleStart.Text = r == 1 and "MEN IN BLACK" or r == 2 and "MYSTERIOUS MEN"
		elseif newText2 == "DELINQUENTS" then
			local r = math.random(1, 3)
			interf.BattleStart.Text = r == 1 and "STREET GOONS" or r == 2 and "INTERFERING CULPRITS" or "DELINQUENTS"
		elseif newText2 == "RICH FIEND" then
            local r = math.random(1, 2)
			interf.BattleStart.Text = r == 1 and "NOUVAEU RICHE" or r == 2 and "RICH FIEND"
		elseif newText2 == "DEVIOUS DAN" then
			interf.BattleStart.Text = "AVERAGE AMERICAN"
		elseif newText2 == "JEFF ATTACK" then
            local r = math.random(1, 2)
			interf.BattleStart.Text = r == 1 and "BLUE BELTS" or r == 2 and "KARATE NERDS"
		elseif newText2 == "GORILLAMAN" then
			local r = math.random(1, 3)
			interf.BattleStart.Text = r == 1 and "THE MONKES" or r == 2 and "APE ARMY" or "GORILLAMAN"
        elseif newText2 == "MOB" then
			local r = math.random(1, 3)
			interf.BattleStart.Text = r == 1 and "SUITED PUNKS" or r == 2 and "OFFICE RECRUITS" or "MOB"    
		elseif newText2 == "HUE" then
			interf.BattleStart.Text = "COPYCAT"
		elseif newText2 == "BLOODSUCKER" then
			local r = math.random(1, 4)
